<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="zineone-script" type="host">*.zineone.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="zineone-connect" type="host">*.zineone.com</value>
                <value id="zineone-wss" type="host">wss://*.zineone.com</value>
            </values>
        </policy>
         <policy id="default-src">
            <values>
                <value id="zineone-default" type="host">*.zineone.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>